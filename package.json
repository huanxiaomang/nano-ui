{
  "name": "nano-ui",
  "version": "1.2.0",
  "private": true,
  "description": "a vue3 UI framework with tailwindcss",
  "type": "module",
  "keywords": [
    "component",
    "vue",
    "tailwindcss"
  ],
  "homepage": "https://github.com/huanxiaomang/nano-ui",
  "bugs": {
    "url": "https://github.com/huanxiaomang/nano-ui/issues"
  },
  "license": "ISC",
  "author": {
    "name": "huanxiaomang",
    "email": "atri2022@163.com",
    "url": "https://github.com/huanxiaomang"
  },
  "main": "index.js",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/huanxiaomang/nano-ui.git"
  },
  "scripts": {
    "dev": "pnpm -C play dev",
    "gen": "bash ./scripts/gc.sh",
    "release": "release-it",
    "docs:dev": "pnpm --filter @nano-ui/docs dev",
    "docs:build": "pnpm --filter @nano-ui/docs build",
    "docs:preview": "pnpm --filter @nano-ui/docs preview",
    "test": "vitest",
    "test:ui": "vitest --ui",
    "test:cov": "vitest --coverage",
    "esm-check": "pnpx are-we-esm --prod",
    "build": "run-s build-hooks build-comp build-utils",
    "build-comp": "cross-env NODE_ENV=production pnpm --filter nano-ui-vue build",
    "build-hooks": "cross-env NODE_ENV=production pnpm --filter @nano-ui/hooks build",
    "build-utils": "cross-env NODE_ENV=production pnpm --filter @nano-ui/build-utils build",
    "build:dev": "cross-env NODE_ENV=development pnpm --filter nano-ui-vue build:watch",
    "lint": "eslint . --ext .vue,.js,.jsx,.ts,.tsx",
    "lint:fix": "eslint . --ext .vue,.js,.jsx,.ts,.tsx --fix",
    "type-check": "vue-tsc --noEmit",
    "ci-test": "vitest run --coverage",
    "verify": "run-p lint type-check ci-test",
    "prepare-release": "run-s verify build",
    "changelog": "npx conventional-changelog -p angular -i CHANGELOG.md -s"
  },
  "peerDependencies": {
    "vue": "^3.2.0"
  },
  "dependencies": {
    "@icestack/cva": "^1.3.8",
    "@nano-ui/components": "workspace:*",
    "@nano-ui/constants": "workspace:*",
    "@nano-ui/hooks": "workspace:*",
    "@nano-ui/locale": "workspace:*",
    "@nano-ui/shared": "workspace:*",
    "@nano-ui/theme": "workspace:*",
    "@popperjs/core": "npm:@sxzz/popperjs-es@^2.11.7",
    "@types/lodash": "^4.14.182",
    "@types/lodash-es": "^4.17.6",
    "@vueuse/core": "^9.1.0",
    "async-validator": "^4.2.5",
    "class-variance-authority": "^0.7.1",
    "lodash": "^4.17.21",
    "lodash-es": "^4.17.21",
    "lodash-unified": "^1.0.2",
    "nano-ui-vue": "workspace:*",
    "shelljs": "^0.8.5",
    "vite": "^6.0.5"
  },
  "devDependencies": {
    "@icestack/tailwindcss": "^1.3.8",
    "@icestack/ui": "^1.3.8",
    "@nano-ui/build": "workspace:^0.0.1",
    "@nano-ui/build-utils": "workspace:^0.0.1",
    "@nano-ui/eslint-config": "workspace:*",
    "@pnpm/find-workspace-packages": "^4.0.16",
    "@pnpm/logger": "^4.0.0",
    "@pnpm/types": "^8.4.0",
    "@release-it/conventional-changelog": "^10.0.0",
    "@types/fs-extra": "^9.0.13",
    "@types/gulp": "^4.0.9",
    "@types/jsdom": "^16.2.14",
    "@types/node": "^22.10.5",
    "@types/shelljs": "^0.8.15",
    "@vitejs/plugin-vue": "^2.3.3",
    "@vitejs/plugin-vue-jsx": "^1.3.10",
    "@vitest/coverage-v8": "^2.0.5",
    "@vitest/ui": "^2.0.5",
    "@vue/test-utils": "^2.0.0",
    "@vue/tsconfig": "^0.1.3",
    "autoprefixer": "^10.4.20",
    "c8": "^7.11.3",
    "chalk": "^5.0.1",
    "concurrently": "^7.2.2",
    "consola": "^2.15.3",
    "conventional-changelog-cli": "^5.0.0",
    "cross-env": "^7.0.3",
    "csstype": "^2.6.20",
    "eslint": "^8.57.1",
    "eslint-define-config": "^1.5.1",
    "expect-type": "^0.13.0",
    "fast-glob": "^3.2.11",
    "happy-dom": "^14.3.3",
    "husky": "^9.0.11",
    "jsdom": "16.4.0",
    "lint-staged": "^13.0.3",
    "npm-run-all2": "^5.0.0",
    "postcss": "^8.4.49",
    "postcss-color-mix": "^1.1.0",
    "postcss-each": "^1.1.0",
    "postcss-each-variables": "^0.3.0",
    "postcss-for": "^2.1.1",
    "postcss-nested": "^6.0.1",
    "puppeteer": "^17.1.3",
    "release-it": "^17.11.0",
    "resize-observer-polyfill": "^1.5.1",
    "rimraf": "^3.0.2",
    "tailwindcss": "^3.4.17",
    "tsx": "^4.7.1",
    "type-fest": "^2.14.0",
    "typescript": "~5.5.4",
    "unplugin-vue-macros": "^2.7.11",
    "vite-plugin-dts": "^4.5.0",
    "vitest": "^2.0.5",
    "vue": "^3.2.37",
    "vue-router": "^4.0.16",
    "vue-tsc": "^2.0.28"
  },
  "engines": {
    "node": ">= 18"
  },
  "simple-git-hooks": {
    "pre-commit": "pnpm lint-staged"
  },
  "lint-staged": {
    "*.{vue,js,ts,jsx,tsx,md,json}": "eslint --fix"
  }
}
